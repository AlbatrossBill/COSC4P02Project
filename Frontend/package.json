{
  "name": "chatbot",
  "version": "0.0.1",
  "description": "Chatbot",
  "main": "dist/chatbot.common.js",
  "files": [
    "dist/*",
    "src/*",
    "package.json"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist",
    "build-bundle": "vue-cli-service build --target lib --name chatbot ./src/chatbot.js",
    "release": "standard-version && git push --follow-tags && npm publish",
    "release:next": "standard-version --prerelease next && git push --follow-tags origin dev && npm publish --tag next",
    "lint": "vue-cli-service lint --fix"
  },
  "keywords": [
    "chatbot"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/jest": "^24.0.11",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^5.1.3",
    "core-js": "^3.3.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.8.3",
    "vue-svg-loader": "^0.15.0",
    "vue-template-compiler": "^2.6.10"
  },
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "axios": "^0.25.0",
    "bootstrap-icons": "^1.8.1",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "vue-clickaway": "^2.2.2",
    "ws": "^8.4.2"
  }
}
